{
    "manifest_version": 3,
    "name": "Notes",
    "description": "description",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "32": "icons/icon.png"
        }
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }
}